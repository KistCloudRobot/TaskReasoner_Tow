PREFIXES:
	PREFIX arbi: <http://www.arbi.com//ontologies#>;

PLAN PERFORM loadPlanPackage(){
	ID : "loadPlanPackage"
	PRECONDITION:
		FACT TaskReasoner($taskReasoner);
	BODY:
		$taskReasoner.loadPlanPackage("TaskReasonerPlanlan/common");
		$taskReasoner.loadPlanPackage("TaskReasonerPlanlan/Demoplan");
		$taskReasoner.loadPlanPackage("TaskReasonerPlanlan/BuiltInPolicy");
		
}